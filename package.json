{
  "name": "qipu",
  "version": "0.0.0",
  "description": "Zettelkasten-inspired knowledge management CLI for LLM workflows",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "qipu": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "zettelkasten",
    "knowledge-management",
    "cli",
    "llm",
    "notes"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "gray-matter": "^4.0.3",
    "toml": "^3.0.0",
    "yaml": "^2.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
